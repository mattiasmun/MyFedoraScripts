#!/bin/bash
eval $(xdotool getmouselocation --shell)
xdotool keyup Return
wmctrl -a "$1"
sleep 0.3
a=$2
a0=5
a1=$((a/a0+1))
a2=0
for((i=0;i<$a1;i++))
do
	a2=$a0
	if (( $a < $a0 ))
	then
		a2=$a
	fi
	for((j=0;j<$a2;j++))
	do
		xdotool mousemove --sync $X $((Y+j*292)) click 1
		sleep 0.2
	done
	xdotool key Next
	sleep 2
	Y=$((Y+208))
	a=$((a-a0))
done
