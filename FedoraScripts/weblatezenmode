#!/bin/bash
eval $(xdotool getmouselocation --shell)
xdotool keyup Return
wmctrl -a "$1"
sleep 0.3
a=$2
a0=5
a2=0
a3=292
a4=1262
a1=$((a*a3/a4+1))
for((i=0;i<$a1 && a>0;i++))
do
	a2=$a0
	if (( $a < $a0 ))
	then
		a2=$a
	fi
	for((j=0;j<$a2;j++))
	do
		xdotool mousemove --sync $X $((Y+j*a3)) click 1
		sleep 0.3
	done
	xdotool key Next
	sleep 5
	if (( $Y < 226 ))
	then
		Y=$((Y+208))
		a0=5
	else
		test $a0 = 5 && Y=$((Y+a3))
		Y=$((Y-83))
		a0=4
	fi
	a=$((a-a0))
done
: '
1262
292*4=1168
1262-1168=94
'
