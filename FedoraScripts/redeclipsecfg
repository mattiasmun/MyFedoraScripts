#!/bin/bash
d=$HOME/.redeclipse
mkdir -p $d
f1=$d/autoexec.cfg
f2=$d/vars.cfg

> $f1

function tof1
{
	echo $1 >> $f1
}

tof1 'bothurtstop 1'
tof1 'botskillmax 7'
tof1 'botskillmin 6'
tof1 'botspeed 0.5'
tof1 'dronemaxcarry 1'
tof1 'dronespeed 0.5'
tof1 'gruntmaxcarry 1'
tof1 'gruntspeed 0.5'
tof1 'jumpspeed 25.0'
tof1 'playerhealth 999999'
tof1 'playerhurtstop 1'
tof1 'playermaxcarry 7'
tof1 'playerweight 5.0'
tof1 'turrethurtstop 1'
tof1 'turretmaxcarry 1'

cp $f1 $f2
