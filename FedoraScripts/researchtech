#!/bin/bash
sleep 1
wmctrl -a "0 A.D."

function type
{
	mstr='brainiac '$1
	echo $mstr
	xclip -i $mstr -selection clipboard
	xdotool key t
	sleep 0.3
	xdotool key --clearmodifiers Ctrl+v
	#xdotool type 'brainiac '
	#xdotool type $1
	sleep 0.03
	xdotool key Return
}

