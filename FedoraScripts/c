#!/bin/bash
cat /etc/motd
string=$(stat -c %y /etc/motd)
string=${string::19}
string='Modified: '$string
echo $string
systemctl list-timers dnf-a* --all
cat /usr/lib/systemd/system/dnf-automatic.timer | grep OnBoot
cat /usr/lib/systemd/system/dnf-automatic.timer | grep OnUnit
dnf check -C
dnf repoquery -C --unneeded
