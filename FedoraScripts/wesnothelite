#!/bin/bash
sleep 1
wmctrl -a "Kampen om Wesnoth"

mymethod
{
	
}

for((i=0;i<$1;i++))
do
	echo 'unit max_hitpoints=500' > $HOME/typestr
	xclip -i $HOME/typestr -selection clipboard
	xdotool key colon
	sleep 0.2
	xdotool key Ctrl+v
#	xdotool type 'iwanttopwnthem '$2
	sleep 0.02
	xdotool key Return
done
